{
  "name": "tsparticles-demo-configs",
  "version": "1.0.6",
  "description": "tsParticles demo configurations",
  "homepage": "https://particles.js.org/",
  "scripts": {
    "prettify:src": "prettier --write ./src",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "compile": "pnpm run build:ts",
    "build:ts": "pnpm run build:ts:browser && pnpm run build:ts:cjs && pnpm run build:ts:esm",
    "build:ts:browser": "tsc -b tsconfig.browser.json",
    "build:ts:cjs": "tsc",
    "build:ts:esm": "tsc -b tsconfig.module.json",
    "bundle": "webpack --mode production",
    "distfiles": "node ./scripts/distfiles.js",
    "rebuild": "pnpm run build",
    "build": "node ./scripts/updateSrc.js && pnpm run clear:dist && pnpm run prettify:src && pnpm run compile && pnpm run lint && pnpm run bundle && pnpm run prettify:readme && pnpm run distfiles",
    "clear:dist": "rimraf -f ./dist",
    "version": "pnpm run distfiles && git add package.dist.json",
    "prepack": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles.git",
    "directory": "utils/configs"
  },
  "keywords": [
    "tsparticles",
    "particles.js",
    "particlesjs",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles-bg",
    "particles-bg-vue",
    "particles-ts",
    "particles.ts",
    "react-particles-js",
    "react-particles.js",
    "react-particles",
    "react",
    "reactjs",
    "vue-particles",
    "ngx-particles",
    "angular-particles",
    "particleground",
    "vue",
    "vuejs",
    "preact",
    "preactjs",
    "jquery",
    "angularjs",
    "angular",
    "typescript",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "confetti",
    "canvas",
    "fireworks",
    "fireworks-js",
    "confetti-js",
    "confettijs",
    "fireworksjs",
    "canvas-confetti"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "author": "Matteo Bruni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/matteobruni"
    },
    {
      "type": "buymeacoffee",
      "url": "https://www.buymeacoffee.com/matteobruni"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-loader": "^8.2.4",
    "browserslist": "^4.20.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.4",
    "webpack": "^5.71.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-tsparticles-plugin": "workspace:^1.0.4"
  },
  "dependencies": {
    "tsparticles-engine": "workspace:^2.0.6"
  }
}
